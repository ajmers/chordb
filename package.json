{
  "name": "chordb",
  "version": "1.0.0",
  "description": "Collect the chords you need.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run webpack-prod",
    "start": "npm run webpack-dev",
    "lint": "eslint src/** --quiet",
    "clean": "find dist/* -print0 | xargs -0 rm -rfv",
    "webpack-dev": "UV_THREADPOOL_SIZE=100 node server.js",
    "webpack-prod": "UV_THREADPOOL_SIZE=100 webpack --config webpack.config.production.js --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ajmers/chordb.git"
  },
  "keywords": [
    "chords",
    "generator"
  ],
  "author": "Anne Maiale",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ajmers/chordb/issues"
  },
  "homepage": "https://github.com/ajmers/chordb#readme",
  "dependencies": {
    "babel-eslint": "^4.1.5",
    "babel-loader": "^5.2.2",
    "babel-polyfill": "^6.2.0",
    "babelify": "^7.2.0",
    "body-parser": "^1.14.2",
    "css-loader": "^0.23.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "jade": "^1.11.0",
    "jquery": "^2.2.0",
    "mongoose": "^4.3.4",
    "morgan": "^1.6.1",
    "node-sass": "^3.4.2",
    "proxy-middleware": "^0.15.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "react-toolbox": "^0.14.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  },
  "devDependencies": {
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^1.7.0",
    "node-sass": "^3.4.2",
    "normalize.css": "^3.0.3",
    "postcss-loader": "^0.8.0",
    "react-addons-css-transition-group": "^0.14.2",
    "react-addons-test-utils": "^0.14.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "toolbox-loader": "0.0.3"
  }
